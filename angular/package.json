{
  "name": "milobella",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:single-spa:menu": "ng build menu --deploy-url https://milobella.com:10443/spa/menu",
    "serve:single-spa:menu": "ng s --project menu --disable-host-check --port 4200 --deploy-url http://localhost:4200/ --live-reload false",
    "serve:dev:menu": "ng s --project menu --configuration=development --port 4200",
    "build:single-spa:login": "ng build login --deploy-url https://milobella.com:10443/",
    "serve:single-spa:login": "ng s --project login --disable-host-check --port 4201 --deploy-url http://localhost:4201/ --live-reload false",
    "serve:dev:login": "ng s --project login --configuration=development --port 4201",
    "build:single-spa:try-it": "ng build try-it --deploy-url https://milobella.com:10443/",
    "serve:single-spa:try-it": "ng s --project try-it --disable-host-check --port 4202 --deploy-url http://localhost:4202/ --live-reload false",
    "build:single-spa:abilities": "ng build abilities --deploy-url https://milobella.com:10443/",
    "serve:single-spa:abilities": "ng s --project abilities --disable-host-check --port 4203 --deploy-url http://localhost:4203/ --live-reload false",
    "build:single-spa:nlg": "ng build nlg --deploy-url https://milobella.com:10443/",
    "serve:single-spa:nlg": "ng s --project nlg --disable-host-check --port 4204 --deploy-url http://localhost:4204/ --live-reload false",
    "serve:dev:nlg": "ng s --project nlg --configuration=development --port 4204"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "12.2.6",
    "@angular/common": "12.2.6",
    "@angular/compiler": "12.2.6",
    "@angular/core": "12.2.6",
    "@angular/forms": "12.2.6",
    "@angular/platform-browser": "12.2.6",
    "@angular/platform-browser-dynamic": "12.2.6",
    "@angular/router": "12.2.6",
    "rxjs": "7.3.0",
    "bootstrap": "5.1.1",
    "single-spa": "5.9.3",
    "single-spa-angular": "5.0.2",
    "tslib": "2.3.1",
    "zone.js": "0.11.4",
    "pixi.js": "6.1.3"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "12.1.2",
    "@angular-devkit/build-angular": "12.2.6",
    "@angular-devkit/build-ng-packagr": "0.1002.0",
    "@angular/cli": "12.2.6",
    "@angular/compiler-cli": "12.2.6",
    "@types/node": "14.17.18",
    "@types/jasmine": "3.9.1",
    "@types/jasminewd2": "2.0.10",
    "codelyzer": "6.0.2",
    "jasmine-core": "3.9.0",
    "jasmine-spec-reporter": "7.0.0",
    "karma": "6.3.4",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-jasmine": "4.0.1",
    "karma-jasmine-html-reporter": "1.7.0",
    "ng-packagr": "12.2.2",
    "protractor": "7.0.0",
    "ts-node": "10.2.1",
    "tslint": "6.1.3",
    "typescript": "4.2.4"
  }
}
