{
  "name": "spa-nlg",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "yarn run serve:single-spa",
    "build": "yarn run build:single-spa",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "postinstall": "ngcc --tsconfig './tsconfig.app.json' --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "build:single-spa": "ng build --prod --deploy-url http://localhost:4205 --output-hashing none",
    "serve:single-spa": "ng serve --port 4205 --deploy-url http://localhost:4205 --serve-path / --live-reload false"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "10.1.6",
    "@angular/common": "10.1.6",
    "@angular/compiler": "10.1.6",
    "@angular/core": "10.1.6",
    "@angular/forms": "10.1.6",
    "@angular/platform-browser": "10.1.6",
    "@angular/platform-browser-dynamic": "10.1.6",
    "@angular/router": "10.1.6",
    "rxjs": "6.6.3",
    "single-spa": "5.7.0",
    "single-spa-angular": "4.5.0",
    "tslib": "2.0.1",
    "zone.js": "0.11.2"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "10.0.1",
    "@angular-devkit/build-angular": "0.1001.3",
    "@angular/cli": "10.1.3",
    "@angular/compiler-cli": "10.1.6",
    "@types/node": "12.12.68",
    "@types/jasmine": "3.5.14",
    "@types/jasminewd2": "2.0.8",
    "codelyzer": "6.0.1",
    "jasmine-core": "3.6.0",
    "jasmine-spec-reporter": "6.0.0",
    "karma": "5.2.3",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-jasmine": "4.0.1",
    "karma-jasmine-html-reporter": "1.5.4",
    "protractor": "7.0.0",
    "ts-node": "9.0.0",
    "tslint": "6.1.3",
    "typescript": "4.0.3"
  }
}
