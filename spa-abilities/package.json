{
  "name": "spa-abilities",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "yarn run serve:single-spa",
    "build": "yarn run build:single-spa",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
